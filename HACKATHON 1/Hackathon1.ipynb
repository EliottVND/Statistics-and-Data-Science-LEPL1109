     "text": [
      "Probabilité estimée = 0.8081616323264653\n"
     ]
    }
   ],
   "source": [
    "#probabilité que le temps d'assemblage de 10 gearboxes soit <= 320\n",
    "n = 0\n",
    "for i in range(0,len(data),10):\n",
    "    time_sum = np.sum(data[i:i+10])\n",
    "    if time_sum<=320 :\n",
    "        n+=1\n",
    "PT320 = 10*n/(len(data))\n",
    "\n",
    "print(\"Probabilité estimée =\",PT320)"
   ]
  },
